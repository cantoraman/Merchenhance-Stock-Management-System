<h2>All Manufacturers</h2>

<table>
  <tr>
    <th>id</th>
    <th>Name</th>
    <th>Website</th>
  </tr>
  <% for manufacturer in @manufacturers %>
  <tr>
    <th><%= manufacturer.id %></th>
    <th><%= manufacturer.name %></th>
    <th> <a href="<%= manufacturer.website %>"><%= manufacturer.website %></a> </th>
  </tr>
    <% end %>

</table>
